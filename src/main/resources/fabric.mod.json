{
  "schemaVersion": 1,
  "id": "fabricgemsproject",
  "version": "${version}",

  "name": "The Gems Project",
  "description": "The Gems Project aims to improve Minecraft's progression system through balanced additions to all categories of game mechanics.",
  "authors": [
    "Fooze (Original Author)",
    "Zachary (Fabric Port)"
  ],
  "contact": {
    "homepage": "https://zachary.lol/",
    "sources": "https://github.com/zacimac/Fabric-Gems-Project"
  },

  "license": "CC0-1.0",
  "icon": "assets/gemsproject/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "lol.zachary.fabricgemsproject.MainInit"
    ],
    "client": [ "lol.zachary.fabricgemsproject.ClientInit" ]
  },
  "mixins": [
    "fabricgemsproject.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
